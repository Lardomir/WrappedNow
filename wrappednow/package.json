{
  "name": "ch.example.wrappednow",
  "displayName": "Wrapped Now",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "esbuild src/app.js --bundle --format=esm --sourcemap --outfile=www/js/bundle.js --watch",
    "build:web": "esbuild src/app.js --bundle --format=esm --minify --outfile=www/js/bundle.js"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^14.0.1",
    "cordova-browser": "^7.0.0",
    "cordova-plugin-advanced-http": "^3.3.1",
    "cordova-plugin-android-permissions": "^1.1.5",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-browsertab": "^0.2.0",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-customurlscheme": "^5.0.2",
    "cordova-plugin-device": "^3.0.0",
    "cordova-plugin-device-motion": "^2.0.1",
    "cordova-plugin-file": "^8.1.3",
    "cordova-plugin-firebasex": "^18.0.7",
    "cordova-plugin-geolocation": "^5.0.0"
  },
  "cordova": {
    "platforms": [
      "browser",
      "android"
    ],
    "plugins": {
      "cordova-plugin-firebasex": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-browsertab": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "ch.example.wrappednow",
        "ANDROID_SCHEME": "ch.example.wrappednow",
        "ANDROID_HOST": "callback",
        "ANDROID_PATHPREFIX": "/"
      },
      "cordova-plugin-geolocation": {},
      "cordova-plugin-device": {},
      "cordova-plugin-device-motion": {},
      "cordova-plugin-android-permissions": {}
    }
  },
  "dependencies": {
    "esbuild": "^0.25.8",
    "firebase": "^12.1.0"
  }
}